{
  "extends": ["standard"],
  "parser": "babel-eslint",
  "env": {
    "jest": true,
    "shared-node-browser": true,
    "es6": true
  },
  "plugins": [
    "flowtype",
    "react",
    "import"
  ],
  "globals": {
    "fetch": true
  },
  "rules": {
    "react/jsx-uses-vars": [2],
    "react/jsx-uses-react": [2],
    "import/no-duplicates": [2],
    "no-duplicate-imports": [0]
  }
}